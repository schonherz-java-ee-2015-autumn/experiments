<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>LoginOrRegister</servlet-name>
		<display-name>LoginOrRegister</display-name>
		<description></description>
		<servlet-class>hu.schonherz.webapp.LoginOrRegister</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserRegister</servlet-name>
		<display-name>UserRegister</display-name>
		<description></description>
		<servlet-class>hu.schonherz.webapp.UserRegister</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginOrRegister</servlet-name>
		<url-pattern>/LoginOrRegister</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserRegister</servlet-name>
		<url-pattern>/UserRegister</url-pattern>
	</servlet-mapping>
	
</web-app>
